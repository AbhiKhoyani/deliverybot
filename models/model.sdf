      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>

    <joint name='caster_back_joint' type='ball'>
      <parent>base_link</parent>
      <child>caster_back_link</child>
    </joint>

  <joint name="cam_left_joint" type="fixed">
    <parent>base_link</parent>
    <child>cam_left_link</child>
    <pose>0.048 0.042 0.182 -1.57 0 -1.57</pose>
    <axis>
